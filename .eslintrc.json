{
    "extends": "airbnb-base",
    "rules": {
        "indent": ["error", 4],
        "complexity": ["error", 5],
        "max-params": ["error", 3],
        "max-statements": ["error", 10],
        "max-depth": ["error", 3],
        "max-len": ["error", { "code": 100 }],
        "class-methods-use-this": 0
    },
    "globals": {
        "moment": true
    },
    "env": {
        "browser": true,
        "jasmine": true,
        "node": true
    }
}
